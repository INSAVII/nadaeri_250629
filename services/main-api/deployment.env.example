# QClick 서비스 기본 가격 설정 (환경변수)
# 이 값들은 배포 시 기본 가격으로 사용됩니다.

# Q네임 서비스 기본 가격 (원)
DEFAULT_QNAME_PRICE=50

# Q텍스트 서비스 기본 가격 (원)
DEFAULT_QTEXT_PRICE=30

# Q캡쳐 서비스 기본 가격 (원)
DEFAULT_QCAPTURE_PRICE=100

# 데이터베이스 설정
DATABASE_URL=postgresql://username:password@localhost/qclick_db

# 서버 포트
PORT=8001

# CORS 설정
CORS_ORIGINS=http://localhost:3003,http://localhost:3001,https://qclick-app.vercel.app

# JWT 시크릿
SECRET_KEY=your-secret-key-here

# 관리자 계정 설정
ADMIN_EMAIL=admin@qclick.com
ADMIN_PASSWORD=admin123 